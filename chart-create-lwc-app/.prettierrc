    "trailingComma": "all",
    "tabWidth": 4,
    "singleQuote": true,
    "overrides": [
        {
            "files": "**/*.html",
            "options": { "parser": "lwc" }
        },
        {
            "files": "*.{cmp,page,component}",
            "options": { "parser": "html" }
        }
    ]
}
